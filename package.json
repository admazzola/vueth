{
  "name": "@vueth/monorepo",
  "version": "1.0.0",
  "keywords": [
    "ethereum",
    "react",
    "uniswap",
    "workspaces",
    "yarn"
  ],
  "private": true,
  "scripts": {
    "vue-app:build": "yarn workspace @vueth/vue-app build --max-old-space-size=12288",
    "vue-app:start": "yarn workspace @vueth/vue-app start",
    "vue-app:test": "yarn workspace @vueth/vue-app test:unit",
    "build": "yarn workspace @vueth/vue-app build --max-old-space-size=12288",
    "chain": "yarn workspace @vueth/buidler chain",
    "node": "yarn workspace @vueth/buidler chain",
    "test": "yarn workspace @vueth/buidler test",
    "start": "yarn workspace @vueth/vue-app start",
    "compile": "yarn workspace @vueth/buidler compile",
    "deploy": "yarn workspace @vueth/buidler deploy",
    "watch": "yarn workspace @vueth/buidler watch",
    "accounts": "yarn workspace @vueth/buidler accounts",
    "balance": "yarn workspace @vueth/buidler balance",
    "send": "yarn workspace @vueth/buidler send",
    "ipfs": "yarn workspace @vueth/vue-app ipfs",
    "surge": "yarn workspace @vueth/vue-app surge",
    "s3": "yarn workspace @vueth/vue-app s3",
    "ship": "yarn workspace @vueth/vue-app ship",
    "generate": "cd packages/buidler && npx buidler generate",
    "account": "cd packages/buidler && npx buidler account",
    "flatten": "cd packages/buidler && npx buidler flatten"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}
